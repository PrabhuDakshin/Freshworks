<%- include("partials/header") -%>

    <div class="container" style="margin-top: 150px;">
        <form action="create" method="post">
        
        <div class="row">
            <div class="col">
                <div class="form-group">
                    <label >Enter Key</label>
                    <input name="key" type="text" class="form-control" id="keyID" autocomplete="off" >
                  </div>
            </div>
        </div>

        <div class="row">
            <div class="col">
                <div class="form-group">
                    <label >Enter Value</label>
                    <textarea name="value" class="form-control" id="valueID" rows="5"></textarea>
                  </div>
            </div>
            <!-- <div class="col">
                <div class="form-group">
                    <label for="value">Enter Value</label>
                    <input type="file"/>
                </div>
            </div> -->
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>

    </form>
    </div>
</body>
</html>